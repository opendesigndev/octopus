{
  "name": "@avocode/octopus-ai",
  "version": "3.0.0-rc.1",
  "description": "\"Octopus AI converter, converts .ai files to Octopus 3+ format.\"",
  "main": "dist/src/index.js",
  "scripts": {
    "create:cleanup": "rimraf temp/input",
    "create:create": "./temp/parser/parse_ai_docker.sh temp/file/$FILE temp/input",
    "create": "yarn create:cleanup && yarn create:create && yarn create:private",
    "create:private": "node node_modules/@avocode/ai-private-data-parser/dist/cli.js temp/file/$FILE temp/privateData/private.json",
    "convert:all": "npx ts-node ./scripts/convert-render.ts",
    "render:test": "npx ts-node ./scripts/render-test.ts",
    "test": "echo 'Tests are not implemented yet'",
    "build": "tsc --build ./tsconfig.json",
    "clean": "rimraf ./node_modules ./lib ./workdir",
    "types:check": "tsc --noEmit",
    "watch": "tsc -w"

  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@avocode/octopus-common": "3.0.0-rc.7",
    "@sentry/node": "6.14.0",
    "dotenv": "10.0.0",
    "lodash": "^4.17.21",
    "paper": "^0.12.15",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "read-pkg-up": "7.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@avocode/ai-private-data-parser": "^0.1.0-alpha.3",
    "@avocode/octopus-ts": "3.0.0-alpha.21",
    "@types/chalk": "^2.2.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "*",
    "@types/pino": "^6.3.3",
    "@types/uuid": "^8.3.1",
    "chalk": "^4.1.2",
    "jest": "^27.3.1",
    "ts-node": "10.4.0"
  }
}
