{
  "name": "@opendesign/octopus-ai",
  "version": "3.0.0-rc.2",
  "description": "Octopus AI converter, converts .ai files to Octopus 3+ format.",
  "license": "Apache-2.0",
  "main": "lib/src/index.js",
  "scripts": {
    "convert:debug": "ts-node ./scripts/convert-render-debug.ts",
    "convert:all": "ts-node ./scripts/convert-design.ts",
    "build": "tsc --build ./tsconfig.json",
    "clean": "rimraf ./node_modules ./lib ./workdir",
    "types:check": "tsc --noEmit",
    "watch": "tsc -w",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "yarn jest",
    "test:integration": "ts-node test/integration",
    "test:integration:update": "ts-node test/integration/update-tests.ts"
  },
  "dependencies": {
    "@opendesign/manifest-ts": "3.0.0-alpha.38",
    "@opendesign/octopus-common": "3.0.0-rc.27",
    "@opendesign/octopus-ts": "3.0.0-alpha.38",
    "@opendesign/illustrator-parser-pdfcpu": "^1.0.4-0",
    "dotenv": "16.0.0",
    "lodash": "^4.17.21",
    "paper": "^0.12.15",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "read-pkg-up": "7.0.1",
    "time-stamp": "^2.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "*",
    "@types/pino": "^6.3.3",
    "@types/rimraf": "^3.0.2",
    "@types/uuid": "^8.3.1",
    "chalk": "^4.1.2",
    "handlebars": "^4.7.7",
    "jest": "^27.3.1",
    "jsondiffpatch": "^0.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "10.4.0",
    "typescript": "4.6.4"
  },
  "files": [
    "lib/src",
    "lib/package.json"
  ]
}
