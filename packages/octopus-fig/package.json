{
  "name": "@opendesign/octopus-fig",
  "version": "3.0.0-rc.50",
  "description": "Figma HTTP API format to Octopus 3+ converter.",
  "license": "Apache-2.0",
  "scripts": {
    "build": "rimraf ./dist && tsc --build tsconfig.json",
    "bundle": "yarn bundle:node && yarn bundle:web && yarn bundle:web-umd && yarn release:prepare",
    "bundle:node": "vite build -c vite.node.js",
    "bundle:web": "vite build -c vite.web.js",
    "bundle:web-umd": "vite build -c vite.web-umd.js",
    "release": "yarn bundle && yarn release:publish",
    "release:prepare": "node ./prepare-release.mjs",
    "release:publish": "yarn publish ./release --non-interactive --ignore-scripts",
    "release:publish:experimental": "yarn publish ./release --non-interactive --ignore-scripts --tag experimental",
    "release:experimental": "yarn bundle && yarn release:publish:experimental",
    "clean": "rimraf ./node_modules ./dist ./workdir ./test/**/report ./release/*",
    "clean:workdir": "rimraf ./workdir/*",
    "convert:api:debug": "ts-node ./examples/node/convert-api-debug.ts",
    "convert:api:local": "ts-node ./examples/node/convert-api-local.ts",
    "convert:plugin:debug": "ts-node ./examples/node/convert-plugin-debug.ts",
    "convert:plugin:local": "ts-node ./examples/node/convert-plugin-local.ts",
    "prepack": "rimraf ./lib && yarn run build",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "yarn jest",
    "test:integration": "yarn test:integration:parser && yarn test:integration:plugin",
    "test:integration:parser": "ts-node test/integration/figma-parser",
    "test:integration:parser:update": "yarn test:integration:parser:update:assets && yarn test:integration:parser:update:tests",
    "test:integration:parser:update:assets": "ts-node test/integration/figma-parser/update-assets.ts",
    "test:integration:parser:update:tests": "ts-node test/integration/figma-parser/update-tests.ts",
    "test:integration:plugin": "ts-node test/integration/squid-plugin",
    "test:integration:plugin:update": "ts-node test/integration/squid-plugin/update-tests.ts",
    "types:check": "tsc --noEmit",
    "typedoc": "typedoc --excludePrivate src/index-node.ts src/index-web.ts",
    "watch": "rimraf ./dist && tsc -w",
    "prepublishOnly": "echo \"Please use 'yarn release' to publish new version\" && exit 1"
  },
  "dependencies": {
    "@opendesign/figma-parser": "3.0.0-rc.17",
    "@opendesign/manifest-ts": "3.1.1",
    "@opendesign/octopus-common": "3.0.0-rc.35",
    "@opendesign/octopus-ts": "3.2.0",
    "@types/lodash": "^4.14.178",
    "@types/uuid": "^8.3.1",
    "dotenv": "16.0.0",
    "eventemitter3": "^5.0.0",
    "image-size": "^1.0.1",
    "lodash": "^4.17.21",
    "paper": "0.12.15",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "*",
    "@types/pino-pretty": "4.7.5",
    "@types/pino-std-serializers": "2.4.1",
    "@types/rimraf": "^3.0.2",
    "chalk": "^4.1.2",
    "enhanced-resolve": "^5.12.0",
    "handlebars": "^4.7.7",
    "jest": "^27.3.1",
    "jsondiffpatch": "^0.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-jest-resolver": "^2.0.0",
    "ts-node": "10.8.0",
    "typedoc": "^0.23.18",
    "typescript": "4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.0.2"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/src/index-node.js",
  "exports": {
    ".": {
      "default": "./dist/src/index-node.js"
    }
  }
}
