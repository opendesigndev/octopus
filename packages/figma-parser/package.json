    "pino-pretty": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/chalk": "^2.2.0",
    "@types/node": "*",
    "@types/pino-pretty": "4.7.5",
    "@types/pino-std-serializers": "2.4.1",
    "chalk": "^4.1.2",
    "jest": "^27.3.1",
    "ts-node": "10.5.0",
    "typescript": "4.6.4",
    "vite": "^2.9.1"
  },
  "files": [
    "lib/src"
