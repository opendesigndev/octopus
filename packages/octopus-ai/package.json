{
  "name": "@opendesign/octopus-ai",
  "version": "3.0.0-rc.5",
  "description": "Adobe Illustrator to Octopus 3+ format converter.",
  "license": "Apache-2.0",
  "keywords": [
    "Octopus 3+",
    "Node.js",
    "Adobe Illustrator",
    "Typescript"
  ],
  "scripts": {
    "convert:debug": "ts-node ./examples/node/convert-debug.ts",
    "convert:local": "ts-node ./examples/node/convert-local.ts",
    "bundle": "yarn run bundle:web",
    "bundle:web": "build -c vite.web-test.js",
    "build": "rimraf ./dist && tsc --build tsconfig.json",
    "release:bundle:node": "vite build -c vite.node.js",
    "release:bundle:web": "vite build -c vite.web.js",
    "release:bundle": "yarn release:bundle:node && yarn release:bundle:web",
    "release:cleanup": "rimraf ./release ./release.tgz",
    "release:publish": "yarn publish release.tgz --non-interactive --ignore-scripts",
    "release": "yarn release:bundle && node ./prepare-release.mjs && yarn release:publish && yarn release:cleanup",
    "clean": "rimraf ./node_modules ./dist ./workdir ./docs",
    "types:check": "tsc --noEmit",
    "watch": "rimraf ./dist && tsc -w",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "vitest run",
    "test:integration": "ts-node test/integration/index.ts",
    "test:integration:update": "ts-node test/integration/update-tests.ts",
    "typedoc": "typedoc --excludePrivate src/index.ts",
    "prepack": "yarn run build"
  },
  "dependencies": {
    "@opendesign/illustrator-parser-pdfcpu": "1.1.2",
    "@opendesign/manifest-ts": "3.0.1",
    "@opendesign/octopus-common": "3.0.0-rc.30",
    "@opendesign/octopus-ts": "3.0.1",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "dotenv": "16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lodash": "^4.17.21",
    "paper": "^0.12.15",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "time-stamp": "^2.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "*",
    "@types/pino-pretty": "4.7.5",
    "@types/pino-std-serializers": "2.4.1",
    "@types/rimraf": "^3.0.2",
    "@types/uuid": "^8.3.1",
    "chalk": "^4.1.2",
    "enhanced-resolve": "^5.12.0",
    "handlebars": "^4.7.7",
    "jsondiffpatch": "^0.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typedoc": "^0.23.18",
    "typescript": "4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.1"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/src/index-node.js",
  "exports": {
    ".": {
      "default": "./dist/src/index-node.js"
    }
  }
}
