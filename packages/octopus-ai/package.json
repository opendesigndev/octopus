{
  "name": "@opendesign/octopus-ai",
  "version": "3.0.0-rc.5",
  "description": "Adobe Illustrator to Octopus 3+ format converter.",
  "license": "Apache-2.0",
  "keywords": [
    "Octopus 3+",
    "Node.js",
    "Adobe Illustrator",
    "Typescript"
  ],
  "scripts": {
    "convert:debug": "ts-node ./examples/node/convert-debug.ts",
    "convert:local": "ts-node ./examples/node/convert-local.ts",
    "build": "rimraf ./dist && node build.js ./tsconfig.json",
    "clean": "rimraf ./node_modules ./dist ./workdir ./docs",
    "types:check": "tsc --noEmit",
    "watch": "tsc -w",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "yarn jest",
    "test:integration": "ts-node test/integration",
    "test:integration:update": "ts-node test/integration/update-tests.ts",
    "typedoc": "typedoc --excludePrivate src/index.ts",
    "prepack": "yarn run build"
  },
  "dependencies": {
    "@opendesign/manifest-ts": "3.0.1",
    "@opendesign/octopus-ts": "3.0.1",
    "@opendesign/octopus-common": "3.0.0-rc.29",
    "@opendesign/illustrator-parser-pdfcpu": "1.1.2",
    "dotenv": "16.0.0",
    "lodash": "^4.17.21",
    "paper": "^0.12.15",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "time-stamp": "^2.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "*",
    "@types/pino": "^6.3.3",
    "@types/rimraf": "^3.0.2",
    "@types/uuid": "^8.3.1",
    "chalk": "^4.1.2",
    "handlebars": "^4.7.7",
    "jest": "^27.3.1",
    "jsondiffpatch": "^0.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "10.4.0",
    "typescript": "4.7.4",
    "typedoc": "^0.23.18"
  },
  "files": [
    "dist/src",
    "dist/package.json"
  ],
  "module": "./dist/src/index.mjs",
  "main": "./dist/src/index.js",
  "exports": {
    ".": {
      "default": {
        "require": "./dist/src/index.js",
        "import": "./dist/src/index.mjs"
      }
    }
  }
}
