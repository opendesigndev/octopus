{
  "name": "@opendesign/octopus-psd",
  "version": "3.0.0-rc.10",
  "description": "Octopus PSD converter, converts .psd files to Octopus 3+ format.",
  "license": "Apache-2.0",
  "scripts": {
    "build": "rimraf ./dist && tsc --build tsconfig.json && node esm.js ./tsconfig.json",
    "clean:workdir": "rimraf ./workdir/*",
    "clean": "rimraf ./node_modules ./dist ./dist ./workdir ./test/integration/report ./jsdoc",
    "convert:debug": "yarn build && node ./dist/examples/node/convert-debug.js",
    "convert:local": "yarn build && node ./dist/examples/node/convert-local.js",
    "prepack": "yarn run build",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "yarn build && yarn node --experimental-vm-modules $(yarn bin jest)",
    "test:integration": "yarn build && node dist/test/integration/index.js",
    "test:integration:update": "yarn build && node dist/test/integration/update-tests.js",
    "types:check": "tsc --noEmit",
    "typedoc": "typedoc --excludePrivate src/index.ts",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@avocode/psd-parser": "0.8.4",
    "@opendesign/manifest-ts": "3.0.1",
    "@opendesign/octopus-common": "3.0.0-rc.29",
    "@opendesign/octopus-ts": "3.0.1",
    "@types/chalk": "^2.2.0",
    "@types/lodash": "^4.14.178",
    "@types/rimraf": "^3.0.2",
    "@types/uuid": "^8.3.1",
    "chalk": "^4.1.2",
    "dotenv": "16.0.0",
    "image-size": "^1.0.1",
    "lodash": "^4.17.21",
    "paper": "0.12.15",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "rimraf": "^3.0.2",
    "time-stamp": "^2.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "*",
    "@types/pino-pretty": "4.7.5",
    "@types/pino-std-serializers": "2.4.1",
    "enhanced-resolve": "^5.12.0",
    "handlebars": "^4.7.7",
    "jest": "^27.3.1",
    "jsondiffpatch": "^0.4.1",
    "typedoc": "^0.23.18",
    "typescript": "4.7.4"
  },
  "files": [
    "dist/src",
    "dist/package.json"
  ],
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "module": "./dist/src/index.mjs",
  "exports": {
    ".": {
      "default": {
        "require": "./dist/src/index.js",
        "import": "./dist/src/index.mjs"
      }
    }
  }
}
