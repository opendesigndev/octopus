{
  "name": "@opendesign/octopus-psd",
  "version": "3.1.0-rc.12",
  "description": "Octopus PSD converter, converts .psd files to Octopus 3+ format.",
  "license": "Apache-2.0",
  "scripts": {
    "bundle": "yarn run bundle:web",
    "bundle:web": "vite build -c vite.web-test.js",
    "build": "rimraf ./dist && tsc --build tsconfig.json",
    "release:bundle:web": "vite build -c vite.web.js",
    "release:bundle:node": "vite build -c vite.node.js",
    "release:bundle": "yarn release:bundle:node && yarn release:bundle:web",
    "release:cleanup": "rimraf ./release",
    "release:publish": "yarn publish ./release --non-interactive --ignore-scripts",
    "release:publish:experimental": "yarn publish ./release --non-interactive --ignore-scripts --tag experimental",
    "release": "yarn release:bundle && node ./prepare-release.mjs && yarn release:publish && yarn release:cleanup",
    "release:experimental": "yarn release:bundle && node ./prepare-release.mjs && yarn release:publish:experimental && yarn release:cleanup",
    "clean:workdir": "rimraf ./workdir/*",
    "clean": "rimraf ./node_modules ./dist ./workdir ./test/integration/report ./jsdoc",
    "convert:debug": "yarn build && node ./dist/examples/node/convert-debug.js",
    "convert:local": "yarn build && node ./dist/examples/node/convert-local.js",
    "prepack": "yarn run build",
    "types:check": "tsc --noEmit",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "vitest run",
    "test:integration": "yarn build && node dist/test/integration/index.js",
    "test:integration:update": "yarn build && node dist/test/integration/update-tests.js",
    "typedoc": "typedoc --excludePrivate src/index.ts",
    "watch": "rimraf ./dist && tsc -w",
    "prepublishOnly": "echo \"Please use 'yarn release' to publish new version\" && exit 1"
  },
  "dependencies": {
    "@opendesign/image-icc-profile-converter": "^0.0.4-experimental",
    "@opendesign/manifest-ts": "3.0.1",
    "@opendesign/octopus-common": "3.0.0-rc.32",
    "@opendesign/octopus-ts": "3.0.1",
    "@types/lodash": "^4.14.178",
    "@types/rimraf": "^3.0.2",
    "@types/uuid": "^8.3.1",
    "@webtoon/psd-ts": "npm:@opendesign/psd-ts@0.5.1-experimental-release-20230222",
    "chalk": "^4.1.2",
    "dotenv": "16.0.0",
    "image-size": "^1.0.1",
    "jimp": "^0.16.2",
    "lodash": "^4.17.21",
    "paper": "0.12.15",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "rimraf": "^3.0.2",
    "time-stamp": "^2.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "*",
    "@types/pino-pretty": "4.7.5",
    "@types/pino-std-serializers": "2.4.1",
    "enhanced-resolve": "^5.12.0",
    "handlebars": "^4.7.7",
    "jsondiffpatch": "^0.4.1",
    "typedoc": "^0.23.18",
    "typescript": "4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.0.2",
    "vitest": "^0.29.1"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "module": "./dist/src/index.js",
  "exports": {
    ".": {
      "default": "./dist/src/index.js"
    }
  }
}
