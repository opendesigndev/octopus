{
  "name": "@opendesign/octopus-xd",
  "version": "3.0.0-rc.21",
  "description": "Octopus XD converter, converts .xd files to Octopus 3+ format.",
  "license": "Apache-2.0",
  "scripts": {
    "bundle": "yarn run bundle:web",
    "bundle:web": "vite build -c vite.web.js && npx vite build -c vite.web.worker.js",
    "clean": "rimraf ./node_modules ./dist ./workdir",
    "convert:all": "ts-node ./examples/node/convert-design.ts",
    "convert:all:debug": "ts-node ./examples/node/convert-design-debug.ts",
    "clean:workdir": "rimraf ./workdir/*",
    "build": "rimraf ./dist && tsc --build tsconfig.json && node esm.js ./tsconfig.json",
    "prepack": "yarn run build",
    "watch": "tsc -w",
    "test": "echo 'Tests are not implemented yet'",
    "typedoc": "typedoc --excludePrivate src/index-node.ts src/index-web.ts",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@opendesign/manifest-ts": "3.0.1",
    "@opendesign/octopus-ts": "3.0.1",
    "@types/blueimp-md5": "^2.18.0",
    "@types/lodash": "^4.14.178",
    "@types/pino": "^6.3.3",
    "@types/uuid": "^8.3.1",
    "@opendesign/octopus-common": "3.0.0-rc.29",
    "blueimp-md5": "^2.19.0",
    "dotenv": "16.0.0",
    "fflate": "0.7.1",
    "lodash": "^4.17.21",
    "paper": "0.12.4",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "time-stamp": "^2.2.0",
    "typescript": "4.7.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/blueimp-md5": "^2.18.0",
    "@types/lodash": "^4.14.178",
    "@types/pino": "^6.3.3",
    "@types/uuid": "^8.3.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/chalk": "^2.2.0",
    "@types/node": "*",
    "chalk": "^4.1.2",
    "jest": "^27.3.1",
    "ts-node": "10.5.0",
    "typedoc": "^0.23.18",
    "vite": "^4.1.1"
  },
  "files": [
    "dist/src",
    "dist/package.json"
  ],
  "module": "./dist/src/index-node.mjs",
  "main": "./dist/src/index-node.js",
  "exports": {
    ".": {
      "node": {
        "require": "./dist/src/index-node.js",
        "import": "./dist/src/index-node.mjs"
      },
      "default": {
        "require": "./dist/src/index-web.js",
        "import": "./dist/src/index-web.mjs"
      }
    }
  }
}
