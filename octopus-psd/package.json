{
  "name": "@avocode/octopus-psd",
  "version": "3.0.0-rc.5",
  "description": "Octopus PSD converter, converts .psd files to Octopus 3+ format.",
  "license": "Apache-2.0",
  "main": "lib/src/index.js",
  "scripts": {
    "build": "tsc --build ./tsconfig.json",
    "clean:workdir": "rimraf ./workdir/*",
    "clean": "rimraf ./node_modules ./lib ./dist ./workdir ./test/integration/report",
    "convert:psd:debug": "ts-node ./example-node/convert-psd-debug.ts",
    "convert:psd:local": "ts-node ./example-node/convert-psd-local.ts",
    "convert:source:debug": "ts-node ./example-node/convert-source-debug.ts",
    "convert:source:local": "ts-node ./example-node/convert-source-local.ts",
    "prepack": "rimraf ./lib && yarn run build",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "yarn jest",
    "test:integration": "ts-node test/integration",
    "test:integration:update": "ts-node test/integration/update-tests.ts",
    "types:check": "tsc --noEmit",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@avocode/manifest-ts": "3.0.0-alpha.37",
    "@avocode/octopus-common": "3.0.0-rc.23",
    "@avocode/octopus-ts": "3.0.0-alpha.37",
    "@avocode/psd-parser": "0.8.4",
    "@sentry/node": "6.14.0",
    "chalk": "^4.1.2",
    "dotenv": "16.0.0",
    "image-size": "^1.0.1",
    "lodash": "^4.17.21",
    "paper": "0.12.15",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "read-pkg-up": "7.0.1",
    "rimraf": "^3.0.2",
    "time-stamp": "^2.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "*",
    "@types/pino": "^6.3.3",
    "@types/rimraf": "^3.0.2",
    "@types/uuid": "^8.3.1",
    "handlebars": "^4.7.7",
    "jest": "^27.3.1",
    "jsondiffpatch": "^0.4.1",
    "ts-jest": "^27.1.3",
    "ts-node": "10.5.0",
    "typescript": "4.6.4"
  },
  "files": [
    "lib/src"
  ]
}
