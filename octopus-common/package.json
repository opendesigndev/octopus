{
  "name": "@avocode/octopus-common",
  "version": "3.0.0-rc.22",
  "license": "UNLICENSED",
  "description": "General utility functions for Octopus 3 common cases.",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./dist/utils/benchmark": {
      "node": "./dist/utils/benchmark-node.js",
      "default": "./dist/utils/benchmark-web.js"
    },
    "./dist/*": "./dist/*.js"
  },
  "scripts": {
    "build": "tsc --build ./tsconfig.json",
    "prepack": "yarn clean-build",
    "clean": "rimraf ./node_modules ./dist",
    "clean-build": "rimraf ./dist && yarn run build",
    "watch": "tsc -w",
    "test": "yarn jest",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@avocode/octopus-ts": "3.0.0-alpha.33",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "*",
    "@types/uuid": "^8.3.1",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "10.4.0",
    "typescript": "4.7.4"
  },
  "files": [
    "dist"
  ]
}
