{
  "name": "@avocode/octopus-xd",
  "version": "3.0.0-rc.18",
  "description": "Octopus XD converter, converts .xd files to Octopus 3+ format.",
  "license": "Apache-2.0",
  "main": "lib/src/index.js",
  "scripts": {
    "bundle": "yarn run bundle:web",
    "bundle:web": "npx vite build -c vite.web.js && npx vite build -c vite.web.worker.js",
    "clean": "npx rimraf ./node_modules ./lib ./workdir",
    "convert:all": "npx ts-node ./scripts/convert-design.ts",
    "convert:all:debug": "npx ts-node ./scripts/convert-design-debug.ts",
    "clean:workdir": "npx rimraf ./workdir/*",
    "build": "tsc --build ./tsconfig.json",
    "prepack": "npx rimraf ./lib && yarn run build",
    "watch": "tsc -w",
    "test": "echo 'Tests are not implemented yet'",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "@avocode/manifest-ts": "3.0.0-alpha.33",
    "@avocode/octopus-common": "3.0.0-rc.23",
    "@avocode/octopus-ts": "3.0.0-alpha.33",
    "@types/blueimp-md5": "^2.18.0",
    "@types/lodash": "^4.14.178",
    "@types/pino": "^6.3.3",
    "@types/uuid": "^8.3.1",
    "blueimp-md5": "^2.19.0",
    "dotenv": "16.0.0",
    "fflate": "^0.7.1",
    "lodash": "^4.17.21",
    "paper": "0.12.4",
    "pino-pretty": "^4.3.0",
    "pino": "^6.7.0",
    "read-pkg-up": "7.0.1",
    "time-stamp": "^2.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/chalk": "^2.2.0",
    "@types/node": "*",
    "chalk": "^4.1.2",
    "jest": "^27.3.1",
    "ts-node": "10.5.0",
    "typescript": "4.6.4",
    "vite": "^2.9.1"
  },
  "files": [
    "lib/src",
    "lib/package.json"
  ],
  "exports": {
    "node": "./lib/src/index-node.js",
    "default": "./lib/src/index-web.js"
  }
}
