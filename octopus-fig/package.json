{
  "name": "@avocode/octopus-fig",
  "version": "3.0.0-rc.6",
  "description": "Octopus Figma converter, converts Figma designs to Octopus 3+ format.",
  "license": "UNLICENSED",
  "main": "dist/src/index.js",
  "scripts": {
    "bundle": "yarn run bundle:web",
    "bundle:web": "npx vite build -c vite.web.js && npx vite build -c vite.web.worker.js",
    "build": "tsc --build ./tsconfig.json",
    "clean:workdir": "rimraf ./workdir/*",
    "clean": "rimraf ./node_modules ./lib ./workdir",
    "convert:debug": "ts-node ./scripts/convert-api-debug.ts",
    "convert:local": "ts-node ./scripts/convert-api-local.ts",
    "parse": "ts-node ./scripts/test-parse.ts",
    "prepack": "rimraf ./lib && yarn run build",
    "test": "vitest run",
    "types:check": "tsc --noEmit",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@avocode/figma-parser": "3.0.0-rc.10",
    "@avocode/manifest-ts": "3.0.0-alpha.36",
    "@avocode/octopus-common": "3.0.0-rc.23",
    "@avocode/octopus-ts": "3.0.0-alpha.33",
    "chalk": "^4.1.2",
    "dotenv": "16.0.0",
    "image-size": "^1.0.1",
    "lodash": "^4.17.21",
    "paper": "0.12.15",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "*",
    "@types/pino": "^6.3.3",
    "@types/rimraf": "^3.0.2",
    "@types/uuid": "^8.3.1",
    "rimraf": "^3.0.2",
    "ts-node": "10.8.0",
    "typescript": "4.6.4",
    "vitest": "^0.23.4"
  },
  "files": [
    "lib/src",
    "lib/package.json"
  ]
}
